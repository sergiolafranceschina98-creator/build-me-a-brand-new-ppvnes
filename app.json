
{
  "expo": {
    "name": "AI Workout Builder",
    "slug": "ai-workout-builder",
    "scheme": "aiworkoutbuilder",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon-jjc.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/final_quest_240x240.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aiworkoutbuilder.app",
      "buildNumber": "1.0.1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aiworkoutbuilder.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "backendUrl": "https://ai-workout-builder-backend.natively.live",
      "resourceClasses": {
        "ios": "m1-medium",
        "android": "medium"
      },
      "eas": {
        "projectId": "c59b1ee6-b1ce-467a-abc7-377810d1fa6c"
      }
    }
  }
}
